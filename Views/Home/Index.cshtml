@{
    ViewData["Title"] = "Home Page";
}

<script id="mapdata" type="application/json">
@Html.Raw(ViewBag.OverpassJson)
</script>

<div class="text-center">
    <h1 class="display-4">Non-Smoking</h1>
    <p>Count: @ViewBag.Overpass.Length</p>
    <p>
        <ul>
        @foreach (LocationViewModel viewModel in ViewBag.Overpass)
        {
            <li>@viewModel.Name</li>
        }
        </ul>
    </p>
    <div id="map" style="width: 100%; height: 800px;"></div>
</div>
